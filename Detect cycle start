class Solution {
public:
    ListNode *detectCycle(ListNode *head) {
    ListNode* fast=head;
    ListNode* slow=head;
    while(fast!=NULL && fast->next!=NULL) 
    {
        fast=fast->next->next;
        slow=slow->next;
        if(slow==fast) break;
    }  
    if(fast==slow)
    {
        if(fast==NULL||fast->next==NULL) return nullptr;
        slow=head;
        while(slow!=fast)
        {
          fast=fast->next;slow=slow->next;
        }
         return slow;
    }
    return nullptr;
    }
};
