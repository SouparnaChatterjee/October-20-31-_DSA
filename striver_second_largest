class Solution {
  public:
    // Function returns the second
    // largest elements
    int getSecondLargest(vector<int> &arr) {
        // Code Here
        int largest=INT_MIN;//imp
        int largest_2=INT_MIN;//imp ... same and INT_MIN
        for(int i=0;i<arr.size();i++)
        {
            if(arr[i]>largest)//else if
            {
                largest_2=largest;
                largest=arr[i];
            }
            else if(arr[i]>largest_2 && arr[i]<largest)//code blocks in order
            largest_2=arr[i];
        }
        if(largest_2!=INT_MIN)//INT_MIN
        return largest_2;
        return -1;
    }
};
